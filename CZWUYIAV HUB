local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "CZWUYIAV HUB " .. Fluent.Version,
    SubTitle = "czwuy_AV",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "" })
}

local Options = Fluent.Options

do
    Fluent:Notify({
        Title = "Notification",
        Content = "This is a notification",
        SubContent = "SubContent",
        Duration = 5
    })
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer

local SelectedPlayer -- ต้องกำหนดจาก dropdown หรือชื่อผู้เล่น

-- Teleport function
local function TeleportToPlayer()
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    if SelectedPlayer and SelectedPlayer.Character and SelectedPlayer.Character:FindFirstChild("HumanoidRootPart") and Character:FindFirstChild("HumanoidRootPart") then
        Character.HumanoidRootPart.CFrame = SelectedPlayer.Character.HumanoidRootPart.CFrame
    end
end

-- Tween function (speed 250)
local function TweenToPlayer()
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    if SelectedPlayer and SelectedPlayer.Character and SelectedPlayer.Character:FindFirstChild("HumanoidRootPart") and Character:FindFirstChild("HumanoidRootPart") then
        local HRP = Character.HumanoidRootPart
        local targetCFrame = SelectedPlayer.Character.HumanoidRootPart.CFrame
        local tweenInfo = TweenInfo.new((HRP.Position - targetCFrame.Position).Magnitude / 250, Enum.EasingStyle.Linear)
        local tween = TweenService:Create(HRP, tweenInfo, {CFrame = targetCFrame})
        tween:Play()
    end
end
local PlayerDropdown = Tabs.Settings:AddDropdown("PlayerDropdown", {
    Title = "Select Player",
    Description = "Choose a player",
    Options = function()
        local list = {}
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer then
                table.insert(list, plr.Name)
            end
        end
        return list
    end,
    Callback = function(Value)
        SelectedPlayer = Players:FindFirstChild(Value)
    end
})

    local Keybind = Tabs.Settings:AddKeybind("Keybind", {
        Title = "KeyBind",
        Mode = "Toggle",
        Default = "LeftControl",
        Callback = function(Value)
            print("Keybind clicked!", Value)
        end,
        ChangedCallback = function(New)
            print("Keybind changed!", New)
        end
    })
Tabs.Settings:AddButton("TeleportButton", {
    Title = "Teleport to Player",
    Description = "Teleport to selected player",
    Callback = TeleportToPlayer
})

Tabs.Settings:AddButton("TweenButton", {
    Title = "Tween to Player",
    Description = "Tween to selected player",
    Callback = TweenToPlayer
})

    Keybind:OnClick(function()
        print("Keybind clicked:", Keybind:GetState())
    end)

    Keybind:OnChanged(function()
        print("Keybind changed:", Keybind.Value)
    end)

    task.spawn(function()
        while true do
            wait(1)
            if Keybind:GetState() then
                print("Keybind is being held down")
            end
            if Fluent.Unloaded then break end
        end
    end)

    Keybind:SetValue("MB2")
    Keybind:SetMode("Toggle")

    Tabs.Settings:AddButton("CreditButton", {
        Title = "Credit: dy czwuyi_AV",
        Description = "Script creator",
        Callback = function()
            Fluent:Notify({
                Title = "Credit",
                Content = "Script by dy czwuyi_AV",
                Duration = 5
            })
        end
    })
end

SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({})
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")
InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)

Window:SelectTab(1)

Fluent:Notify({
    Title = "dy czwuyi_AV",
    Content = "The script has been loaded.",
    Duration = 8
})
